{
  "name": "@s809/noisecord",
  "version": "0.1.78",
  "repository": "github:s809/noisecord",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/noisecord.d.ts",
  "files": [
    "./dist/**/*.js",
    "./dist/noisecord.d.ts",
    "./dist/tsdoc-metadata.json"
  ],
  "scripts": {
    "api": "npm run build && api-extractor run --local && ! grep -w Warning ./etc/noisecord.api.md && typedoc",
    "build": "tsc",
    "checkall": "clear && npm test && npm run api && madge --circular ./src/index.ts",
    "clean": "rimraf ./dist",
    "prepack": "npm run build && api-extractor run",
    "test": "mocha",
    "test:cov": "c8 npm test"
  },
  "c8": {
    "all": true,
    "check-coverage": true,
    "exclude": [
      "src/**/*.d.ts",
      "src/**/*.spec.ts",
      "src/**/testData/**"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "text",
      "text-summary",
      "lcov"
    ]
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.6.0",
    "@types/string-format": "^2.0.0",
    "discord.js": "^14.13.0",
    "flat": "^5.0.2",
    "lodash-es": "^4.17.21",
    "map-obj": "^5.0.2",
    "string-format": "^2.0.0",
    "type-fest": "github:sindresorhus/type-fest",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.23.0",
    "@microsoft/api-extractor": "^7.37.0",
    "@types/chai": "^4.3.6",
    "@types/chai-as-promised": "^7.1.6",
    "@types/chai-subset": "^1.3.3",
    "@types/flat": "^5.0.3",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.16",
    "@types/sinon-chai": "^3.2.9",
    "c8": "^8.0.1",
    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "chai-exclude": "^2.1.0",
    "chai-subset": "^1.6.0",
    "madge": "^6.1.0",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.1",
    "sinon": "^16.0.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
