{
  "name": "noisecord",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./build",
    "test": "mocha",
    "test:cov": "c8 npm test"
  },
  "c8": {
    "all": true,
    "check-coverage": true,
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/testData/**"
    ],
    "include": [
      "**/*.ts"
    ],
    "reporter": [
      "text",
      "text-summary"
    ]
  },
  "dependencies": {
    "discord.js": "^14.5.0",
    "lodash-es": "^4.17.21",
    "rimraf": "^3.0.2",
    "sinon-chai": "^3.7.0",
    "string-format": "^2.0.0",
    "type-fest": "^3.5.4",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-subset": "^1.3.3",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.3",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.9",
    "@types/string-format": "^2.0.0",
    "c8": "^7.12.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-exclude": "^2.1.0",
    "chai-subset": "^1.6.0",
    "mocha": "^10.1.0",
    "sinon": "^14.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1"
  }
}
